{
  "name": "gatsby-vim",
  "private": true,
  "description": "A minimal, Vim inspired Gatsby theme, following the Gruvbox color scheme.",
  "version": "0.1.0",
  "author": "Alexandros Vasileiou <alex@alexandrosvasileiou.com>",
  "dependencies": {
    "gatsby": "^2.25.3",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-offline": "^3.3.3",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-source-filesystem": "^2.4.2",
    "gatsby-transformer-sharp": "^2.5.21",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-plugin-babel": "^5.3.1",
    "prettier": "2.1.2",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "gatsby",
    "vim",
    "gruvbox"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexandrosVasileiou/gatsby-vim"
  },
  "bugs": {
    "url": "https://github.com/AlexandrosVasileiou/gatsby-vim/issues"
  }
}
